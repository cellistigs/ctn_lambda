{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::yass-test"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "yassdevelopers/",
            "yassdevelopers/inputs",
            "yassdevelopers/results",
            "logs",
            "yassdevelopers/submissions",
            "yassdevelopers/configs",
            "yassdevelopers/inputs/",
            "yassdevelopers/results/",
            "yassdevelopers/submissions/",
            "yassdevelopers/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::yass-test"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "yassdevelopers/inputs/*",
            "yassdevelopers/results/*",
            "yassdevelopers/configs/*",
            "yassdevelopers/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::yass-test/yassdevelopers/inputs/*",
        "arn:aws:s3:::yass-test/yassdevelopers/configs/*",
        "arn:aws:s3:::yass-test/yassdevelopers/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::yass-test/yassdevelopers/results/*",
        "arn:aws:s3:::yass-test/yassdevelopers/configs/*"
      ]
    }
  ]
}