{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::john-dev-test"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "debugger/",
            "debugger/inputs",
            "debugger/results",
            "logs",
            "debugger/submissions",
            "debugger/configs",
            "debugger/inputs/",
            "debugger/results/",
            "debugger/submissions/",
            "debugger/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::john-dev-test"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "debugger/inputs/*",
            "debugger/results/*",
            "debugger/configs/*",
            "debugger/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::john-dev-test/debugger/inputs/*",
        "arn:aws:s3:::john-dev-test/debugger/configs/*",
        "arn:aws:s3:::john-dev-test/debugger/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::john-dev-test/debugger/results/*",
        "arn:aws:s3:::john-dev-test/debugger/configs/*"
      ]
    }
  ]
}