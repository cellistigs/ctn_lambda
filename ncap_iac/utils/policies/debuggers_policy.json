{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::pmd-ncap-web"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "debuggers/",
            "debuggers/inputs",
            "debuggers/results",
            "logs",
            "debuggers/submissions",
            "debuggers/configs",
            "debuggers/inputs/",
            "debuggers/results/",
            "debuggers/submissions/",
            "debuggers/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::locanmf-ncap-web"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "debuggers/inputs/*",
            "debuggers/results/*",
            "debuggers/configs/*",
            "debuggers/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::locanmf-ncap-web/debuggers/inputs/*",
        "arn:aws:s3:::locanmf-ncap-web/debuggers/configs/*",
        "arn:aws:s3:::locanmf-ncap-web/debuggers/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::locanmf-ncap-web/debuggers/results/*",
        "arn:aws:s3:::locanmf-ncap-web/debuggers/configs/*"
      ]
    }
  ]
}