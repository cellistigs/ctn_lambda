{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": "s3:GetBucketLocation",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Allow",
      "Action": "s3:HeadBucket",
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    },
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::yass-test"
      ],
      "Condition": {
        "StringEquals": {
          "s3:prefix": [
            "",
            "developergroup/",
            "developergroup/inputs",
            "developergroup/results",
            "logs",
            "developergroup/submissions",
            "developergroup/configs",
            "developergroup/inputs/",
            "developergroup/results/",
            "developergroup/submissions/",
            "developergroup/configs/"
          ],
          "s3:delimiter": [
            "/"
          ]
        }
      }
    },
    {
      "Sid": "ListSubBucket",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::yass-test"
      ],
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "developergroup/inputs/*",
            "developergroup/results/*",
            "developergroup/configs/*",
            "developergroup/submissions/*"
          ]
        }
      }
    },
    {
      "Sid": "Inputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::yass-test/developergroup/inputs/*",
        "arn:aws:s3:::yass-test/developergroup/configs/*",
        "arn:aws:s3:::yass-test/developergroup/submissions/*"
      ]
    },
    {
      "Sid": "Outputfolderwrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::yass-test/developergroup/results/*",
        "arn:aws:s3:::yass-test/developergroup/configs/*"
      ]
    }
  ]
}